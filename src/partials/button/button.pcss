.button {
  display: block;
  background: blue;
  width: 220px;
  padding: 10px 0;
  border-radius: 20px;
  text-decoration: none;
  text-align: center;
  color: white;
}
.button-back {
  position: absolute;
  top: 0;
  left: 0;
  width: 55px;
  height: 100%;
  border: none;
  border-right: 1px #999 solid;
  background: url(../../../static/back.svg) center/35px no-repeat;
  background-color: #eee;
}