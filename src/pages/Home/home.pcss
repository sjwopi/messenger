.nav {
  width: 280px;
  margin: 100px auto;
  align-items: center;
  &__list {
    height: 650px;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    align-items: center;
    padding: 0;
    list-style: none;
  }
  &__item {
    width: 280px;
    height: 60px;
    border-radius: 30px;
    font-size: 22px;
    font-weight: 700;
    color: white;
  }
}
